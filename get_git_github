__author__ = 'Dell'
import requests
from bs4 import BeautifulSoup




url = 'https://github.com/dahlbyk/posh-git'
r = requests.get(url)

soup = BeautifulSoup(r.content)

get_data = soup.find_all('input')


for link in get_data:
    ls.append(link.get('value'))
    for item in ls:
        print item
        if '.git' in item:
            print item

